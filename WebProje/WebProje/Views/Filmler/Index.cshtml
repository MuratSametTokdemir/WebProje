
@{
    ViewData["Title"] = "Index";
}
@model List<Filmler>
<h1>Filmler</h1>
<table class="table"> 
    <thead>
        <tr>
            <th>Name</th>
            <th>Year</th>
            <th>Director Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach( var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                   <td>@item.Year</td>
                      <td>@item.DirectorName</td>

            </tr>
        }           
    </tbody>
</table>

